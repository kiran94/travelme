@model IList<com.kiransprojects.travelme.Framework.Entities.Trip>
@using com.kiransprojects.travelme.Extensions
@using com.kiransprojects.travelme.Framework.Entities.Trip

@if (Model.HasContent<Trip>())
{
    @for (int i = 0; i < Model.Count; i++)
    {
        @Html.DisplayFor(model => model[0].TripName)
        <hr />
        @Html.DisplayFor(model => model[0].TripDescription)
        <hr />
    }
}
else
{
    <p>No Trips</p>
}