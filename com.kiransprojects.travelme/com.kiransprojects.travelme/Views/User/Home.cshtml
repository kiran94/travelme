@model com.kiransprojects.travelme.Framework.Entities.UserEntity
@{
    ViewBag.Title = "Home";
    ViewBag.Name = Model.FirstName;
}

@Html.Partial("Navigation")

<div class="row" id="head">

    <div class="col-xs-12 col-sm-4">
        <div class="panels">

            <h3>Trips</h3>

            @if (Model.Trips != null && Model.Trips.Count > 0)
            {
                for (int i = 0; i < Model.Trips.Count; i++)
                {
                    <h4>@Model.Trips[i].TripName</h4>
                }
            }
            else
            {
                <h4>No Trips!</h4>
            }
        </div>
    </div>

    <div class="col-xs-12 col-sm-4">
        <div class="panels">
            <h3>Posts</h3>
        </div>
    </div>


    <div class="col-xs-12 col-sm-4">
        <div class="panels">
            <h3>Locations</h3>
        </div>
    </div>




</div>





