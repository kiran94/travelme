@model IList<com.kiransprojects.travelme.Framework.Entities.Trip>
@using com.kiransprojects.travelme.Framework.Entities.Trip
@{
    ViewBag.Title = "Trips";
}

@Html.Partial("Navigation")

<div class="row" id="banner">
    <div class="col-xs-12">
        <h1>@ViewBag.Title</h1>
    </div>
</div>

<div class="row">
    <div class="col-xs-12 col-xs-push-12 col-sm-6">
       @Html.Partial("PartialListTrips", Model)
    </div>
</div>

<div class="row">
    <div class="col-xs-12 col-xs-pull-12 col-sm-6">
        @Html.Partial("PartialAddTrip", new Trip())
    </div>
</div>